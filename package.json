{
  "name": "serverless-terraform-fetcher",
  "version": "1.0.0",
  "description": "Fetches Terraform patterns from AWS Serverless Land using Node.js, Express, tRPC and Zod",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "fetch": "tsx src/fetcher.ts",
    "scrape": "node dist/run-scraper.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.js"
  },
  "dependencies": {
    "@effect/schema": "^0.75.5",
    "@octokit/graphql": "^9.0.1",
    "@octokit/rest": "^21.0.2",
    "@trpc/server": "^10.45.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "effect": "^3.17.6",
    "express": "^4.18.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-plugin-functional": "^9.0.2",
    "tdd-guard-vitest": "^0.1.3",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
