{
  "name": "serverless-terraform-fetcher",
  "version": "1.0.0",
  "description": "Fetches Terraform patterns from AWS Serverless Land using Node.js, Express, tRPC and Zod",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "fetch": "tsx src/fetcher.ts",
    "scrape": "node dist/run-scraper.js",
    "scrape:terraform": "tsx src/scraper-runner.ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint . --ext .ts,.js",
    "docs:generate": "npm run build && npx tsx src/generate-docs.ts",
    "docs:serve": "mkdocs serve --dev-addr 127.0.0.1:8000",
    "docs:build": "mkdocs build",
    "docs:dev": "npm run docs:generate && npm run docs:serve"
  },
  "dependencies": {
    "@effect/schema": "^0.75.5",
    "@octokit/graphql": "^9.0.1",
    "@octokit/rest": "^21.0.2",
    "@trpc/server": "^10.45.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "effect": "^3.17.6",
    "express": "^4.18.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-plugin-functional": "^9.0.2",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "mkdocs-material": "^0.0.1-security",
    "tdd-guard-jest": "^0.1.1",
    "ts-jest": "^29.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
